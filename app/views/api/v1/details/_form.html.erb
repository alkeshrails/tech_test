<%= form_with(model: detail, local: true, data: { turbo_frame: 'details_list' }) do |form| %>
  <div class="field">
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div class="field">
    <%= form.label :person_id %>
    <%= form.number_field :person_id %>
  </div>
  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
